
def multiply_transpose(a, b) {
  return mat_add(a, b);
}

def main() {
  var a<2, 3> = [10, 20, 30, 40, 50, 60];
  var b<2, 3> = [80, 20, 10, 20, 40, 20];
  var tr_res = multiply_transpose(a, b);
  print(tr_res);

  var first<2, 3> = [1, 2, 3, 4, 5, 6];
  var second<2, 3> = [8, 2, 1, 2, 4, 2];

  var res = mat_add(transpose(first), transpose(second));

  print(res);
}

